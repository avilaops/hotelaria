{
  "version": "1.0",
  "appSettings": {
    "ASPNETCORE_ENVIRONMENT": "Production",
    "WEBSITES_PORT": "8080",
    "WEBSITE_TIME_ZONE": "E. South America Standard Time",
    "WEBSITE_HTTPLOGGING_RETENTION_DAYS": "7"
  },
  "healthCheck": {
    "path": "/health",
    "interval": "00:01:00"
  },
  "deployment": {
    "keepAlive": true,
    "restartOnDeploy": true
  }
}
